{
    "name": "a-h-abid/eloquent-cassandra",
    "description": "Cassandra driver for Eloquent ORM",
    "keywords": ["laravel","lumen","eloquent","cassandra","csql","database","model"],
    "homepage": "https://github.com/a-h-abid/eloquent-cassandra",
    "authors": [
        {
            "name": "Ahmedul Haque Abid",
            "role": "Developer",
            "homepage": "https://github.com/a-h-abid",
            "email": "a_h_abid@hotmail.com"
        }
    ],
    "license" : "MIT",
    "require": {
        "php": ">=7.2.0",
        "ext-cassandra": "*",
        "illuminate/support": "9.*",
        "illuminate/container": "9.*",
        "illuminate/database": "9.*",
        "illuminate/events": "9.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 | ^9.0",
        "orchestra/testbench": "^3.1",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "AHAbid\\EloquentCassandra\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AHAbid\\EloquentCassandra\\Tests\\" : "tests/",
            "AHAbid\\EloquentCassandra\\Fixtures\\" : "tests/fixtures/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
